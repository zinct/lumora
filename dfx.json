{
  "canisters": {
    "backend": {
      "dependencies": ["token", "storage", "nft"],
      "main": "src/backend/main.mo",
      "type": "motoko"
    },
    "token": {
      "main": "src/backend/token/token.mo",
      "type": "motoko"
    },
    "nft": {
      "dependencies": ["token"],
      "main": "src/backend/NFT/NFT.mo",
      "type": "motoko"
    },
    "chatbot": {
      "dependencies": ["llm"],
      "main": "src/backend/chatbot/chatbot.mo",
      "type": "motoko"
    },
    "llm": {
      "type": "custom",
      "candid": "https://github.com/dfinity/llm/releases/latest/download/llm-canister-ollama.did",
      "wasm": "https://github.com/dfinity/llm/releases/latest/download/llm-canister-ollama.wasm"
    },
    "frontend": {
      "dependencies": ["backend", "face_recognition"],
      "source": ["src/frontend/dist"],
      "type": "assets",
      "workspace": "frontend"
    },
    "storage": {
      "type": "assets",
      "source": ["src/assets"],
      "build": "echo 'skip build'"
    },
    "face_recognition": {
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "candid": "src/backend/face_recognition/face_recognition.did",
      "package": "face_recognition",
      "type": "custom",
      "wasm": "target/wasm32-wasip1/release/face_recognition-ic.wasm",
      "build": ["bash build-face-recognition.sh"]
    },
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai",
          "playground": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
